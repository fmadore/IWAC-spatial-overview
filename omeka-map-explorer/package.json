{
	"name": "iwac-spatial-overview",
	"private": true,
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check .",
		"build:data:networks": "pwsh -NoLogo -NoProfile -Command if (Get-Command py -ErrorAction SilentlyContinue) { py -3 ../scripts/build_networks.py } elseif (Get-Command python -ErrorAction SilentlyContinue) { python ../scripts/build_networks.py } else { Write-Error 'Python not found. Install Python 3.x or ensure py/python is on PATH.' }",
		"test:e2e": "playwright test",
		"test": "npm run test:unit -- --run",
		"test:unit": "vitest",
		"test:ci": "vitest --run"
	},
	"devDependencies": {
		"@internationalized/date": "^3.8.2",
		"@lucide/svelte": "^0.515.0",
		"@playwright/test": "^1.54.2",
		"@sveltejs/adapter-auto": "^4.0.0",
		"@sveltejs/adapter-static": "^3.0.0",
		"@sveltejs/kit": "^2.31.1",
		"@sveltejs/vite-plugin-svelte": "^5.1.1",
		"@tailwindcss/postcss": "^4.1.12",
		"@tailwindcss/typography": "^0.5.16",
		"@testing-library/jest-dom": "^6.7.0",
		"@testing-library/svelte": "^5.2.8",
		"@types/d3-cloud": "^1.2.9",
		"@types/d3-scale": "^4.0.9",
		"@types/d3-scale-chromatic": "^3.1.0",
		"@types/leaflet": "^1.9.20",
		"autoprefixer": "^10.4.20",
		"bits-ui": "^2.9.3",
		"clsx": "^2.1.1",
		"jsdom": "^26.1.0",
		"postcss": "^8.5.6",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.38.1",
		"svelte-check": "^4.3.1",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^1.0.0",
		"tailwindcss": "^4.1.12",
		"tw-animate-css": "^1.3.7",
		"typescript": "^5.9.2",
		"vite": "^6.3.5",
		"vitest": "^3.2.4"
	},
	"dependencies": {
		"@sigma/layer-leaflet": "^3.0.0",
		"@sigma/node-border": "^3.0.0",
		"@sigma/node-square": "^3.0.0",
		"@sigma/utils": "^3.0.0",
		"axios": "^1.11.0",
		"d3-array": "^3.2.4",
		"d3-axis": "^3.0.0",
		"d3-cloud": "^1.2.7",
		"d3-force": "3.0.0",
		"d3-scale": "^4.0.2",
		"d3-scale-chromatic": "^3.1.0",
		"d3-selection": "^3.0.0",
		"d3-shape": "^3.2.0",
		"d3-time": "^3.1.0",
		"d3-time-format": "^4.1.0",
		"graphology": "^0.25.4",
		"graphology-layout": "^0.6.1",
		"graphology-layout-forceatlas2": "^0.7.1",
		"graphology-layout-noverlap": "^0.4.0",
		"leaflet": "^1.9.4",
		"lucide-svelte": "^0.542.0",
		"maplibre-gl": "^5.6.2",
		"papaparse": "^5.5.3",
